{
  "name": "rocket/start",
  "description": "The Lightweight skeleton for easy start app",
  "keywords": [
    "framework"
  ],
  "license": "MIT",
  "type": "project",
  "version": "1.3.0",
  "require": {
    "php": "^8.3",
    "ext-bcmath": "*",
    "ext-calendar": "*",
    "ext-curl": "*",
    "ext-igbinary": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-sockets": "*",
    "ext-zip": "*",
    "ext-swoole": "*",
    "ext-zlib": "*",
    "darkaonline/swagger-lume": "^11.0",
    "hhxsv5/laravel-s": "^3.8",
    "illuminate/redis": "^11.0",
    "laravel-doctrine/orm": "^3.0",
    "laravel/lumen-framework": "^11.0",
    "laravel/socialite": "^5.0",
    "league/flysystem-aws-s3-v3": "^3.29",
    "pda/pheanstalk": "^6.0",
    "socialiteproviders/apple": "^5.0",
    "socialiteproviders/google": "^4.0",
    "symfony/mailer": "^7.0",
    "tymon/jwt-auth": "^2.0"
  },
  "require-dev": {
    "flipbox/lumen-generator": "^9.3",
    "friendsofphp/php-cs-fixer": "^3.0",
    "laravel-doctrine/migrations": "^3.4",
    "mockery/mockery": "^1.6",
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": "^10.0|^11.0",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.0",
    "swoole/ide-helper": "^5"
  },
  "autoload": {
    "files": [
      "src/Core/_helpers.php"
    ],
    "psr-4": {
      "Core\\": "src/Core/",
      "Infrastructure\\": "src/Infrastructure/",
      "Module\\": "src/Modules/"
    }
  },
  "autoload-dev": {
    "Tests\\": "src/Tests/"
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('src/.env') || copy('src/.env.example', 'src/.env');\"",
      "@php -r \"file_exists('ex.Homestead.yaml') || copy('ex.Homestead.yaml', 'Homestead.yaml');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform-check": false,
    "platform": {
      "php": "8.3"
    },
    "github-oauth": {
    }
  },
  "provide": {
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-zlib": "*",
    "ext-redis": "*",
    "ext-pcntl": "*",
    "ext-swoole": "*",
    "ext-bcmath": "*",
    "ext-imagick": "*",
    "ext-openssl": "*",
    "ext-sockets": "*",
    "ext-igbinary": "*",
    "ext-mbstring": "*",
    "ext-calendar": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
